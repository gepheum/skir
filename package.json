{
  "name": "skir",
  "version": "1.1.2",
  "description": "",
  "homepage": "https://skir.sh/",
  "bugs": {
    "url": "https://github.com/gepheum/skir/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gepheum/skir.git"
  },
  "license": "ISC",
  "author": "Tyler Fibonacci <gepheum@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./dist/*": {
      "import": "./dist/*",
      "require": "./dist/*"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "bin": "./dist/compiler.js",
  "files": [
    "dist",
    "src",
    "!**/*.test.ts"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "skir": "npm run build && node ./dist/compiler.js",
    "test": "mocha",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "glob": "^13.0.6",
    "skir-cc-gen": "^1.0.7",
    "skir-dart-gen": "^1.0.4",
    "skir-internal": "^0.2.4",
    "skir-java-gen": "^1.0.5",
    "skir-kotlin-gen": "^1.0.6",
    "skir-python-gen": "^1.0.4",
    "skir-typescript-gen": "^1.0.6",
    "watcher": "^2.3.1",
    "yaml": "^2.7.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "buckwheat": "^1.1.2",
    "eslint": "^10.0.1",
    "mocha": "^11.7.5",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.2.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.56.0"  },
  "overrides": {
    "minimatch": "^10.2.1",
    "diff": "^8.0.0"  }
}
